{
    "name": "forkeringas/page-tree-crud",
    "type": "library",
    "description": "An admin panel for pages with sections, using Backpack CRUD on Laravel 9",
    "keywords": [
        "backpack",
        "PageTreeCRUD"
    ],
    "homepage": "https://github.com/forkeringas/PageTreeCRUD",
    "license": "proprietary",
    "authors": [
        {
            "name": "forkeringas",
            "email": "admin@admin.com",
            "homepage": "https://github.com/forkeringas/PageTreeCRUD",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "backpack/crud": "^5.6",
        "cviebrock/eloquent-sluggable": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^10.0||^9.0||^7.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3||~3.0"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\PageTreeCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\PageTreeCrud\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\PageTreeCRUD\\PageTreeCRUDServiceProvider"
            ]
        }
    }
}
